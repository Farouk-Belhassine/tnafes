<!DOCTYPE html>

<title>Participation</title>

{% block body %}
    <h1>Participation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>IdParticipation</th>
                <td>{{ participation.idParticipation }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ participation.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ participation.prenom }}</td>
            </tr>
            <tr>
                <th>EMail</th>
                <td>{{ participation.eMail }}</td>
            </tr>
            <tr>
                <th>NbParticipant</th>
                <td>{{ participation.nbParticipant }}</td>
            </tr>
            <tr>
                <th>DatePart</th>
                <td>{{ participation.datePart ? participation.datePart|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateEvenement</th>
                <td>{{ participation.dateEvenement ? participation.dateEvenement|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('participation_index') }}">back to list</a>

    <a href="{{ path('participation_edit', {'idParticipation': participation.idParticipation}) }}">edit</a>

    {{ include('participation/_delete_form.html.twig') }}
{% endblock %}
