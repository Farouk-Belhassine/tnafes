<form method="post" action="{{ path('abonnement_delete', {'idAbonnement': abonnement.idAbonnement}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cet abonnement?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ abonnement.idAbonnement) }}">
    <button class="btn">Delete</button>
</form>
